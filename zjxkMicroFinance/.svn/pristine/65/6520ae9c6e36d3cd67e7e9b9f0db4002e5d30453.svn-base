<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.yzframework.model">
    <class name="com.yzframework.model.Mcardloanform" table="M_CARDLOAN_FORM">
        <comment>车易贷申请单</comment>
        <id name="id" type="java.lang.String">
            <column name="ID" length="32">
                <comment>ID</comment>
            </column>
            <generator class="uuid.hex"/>
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME">
                <comment>姓名</comment>
            </column>
        </property>
        <property name="idcard" type="java.lang.String">
            <column name="IDCARD" length="32">
                <comment>身份/企业识别码</comment>
            </column>
        </property>
        <property name="age" type="java.lang.String">
            <column name="AGE">
                <comment>年龄</comment>
            </column>
        </property>
        <property name="address" type="java.lang.String">
            <column name="ADDRESS">
                <comment>地址</comment>
            </column>
        </property>
        <property name="companyname" type="java.lang.String">
            <column name="COMPANYNAME">
                <comment>公司名称</comment>
            </column>
        </property>
        <property name="status" type="java.lang.String">
            <column name="STATUS" length="1">
                <comment>状态</comment>
            </column>
        </property>
        <property name="createid" type="java.lang.String">
            <column name="CREATEID" length="32">
                <comment>创建者ID</comment>
            </column>
        </property>
        <property name="createtime" type="java.sql.Timestamp">
            <column name="CREATETIME">
                <comment>创建时间</comment>
            </column>
        </property>
        <property name="updateid" type="java.lang.String">
            <column name="UPDATEID" length="32">
                <comment>更新者ID</comment>
            </column>
        </property>
        <property name="updatetime" type="java.sql.Timestamp">
            <column name="UPDATETIME">
                <comment>更新时间</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>