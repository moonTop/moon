body{font-size:12px; font-family:Microsoft YaHei,\5b8b\4f53,Tahoma,Sans-serif; color: #364260; background-color:#9f9f9f;}
ol,ul,li {list-style:none;margin: 0px;padding: 0px;}

.divForm
{
	margin:0px auto;
	width:980px;
	background-color:White;
}

.divHead
{	width:980px;
	height:85px;
	background:url(../imgs/Bg_Head_Corp.gif) no-repeat;
	float:left;
}

.divMenuMain
{
	width:980px;
	height:36px;
	background:url(../imgs/Bg_MenuBig_Corp.gif) no-repeat;
	padding-left:20px;
	float:left;	
}

.divMenuSub
{
	width:980px;
	height:26px;
	background:url(../imgs/Bg_MenuSmall_Corp.gif) no-repeat;
	float:left;
}

.divInfoBar
{
	width:944px;
	height:35px;
	border:solid 1px #F3E2B6;
	background:url(../imgs/Bg_InfoBar_Corp.gif) repeat-x;
	float:left;
}

.divLoginBox
{
	width:946px;
	height:285px;
	float:left;
}

.divContent
{
	width:980px;
	background:url(../imgs/Bg_Content_Corp.gif) repeat-y;
	padding:10px 0 10px 20px;
}

.divBottom
{
	width:980px;
	height:50px;
	background:url(../imgs/Bg_Bottom_Corp.gif) no-repeat;
	text-align:center;
	color:#555555;
	padding-top:10px;
	float:left;
}

.linkNormal,linkNormal:link,linkNormal:visited,linkNormal:hover
{
	 color:#555555;
	 text-decoration:none;
}

.MenuNotChoise
{
	float:left;
	width:150px;
	line-height:36px;
	text-align:center;
	margin-right:-2px;
	margin-left:-2px;
}

.MenuNotChoise a
{
	display:block;
	color:#FFFFFF;
	width:150px;
	text-decoration:none;
}

.MenuChoise
{
	float:left;
	width:150px;
	line-height:36px;
	text-align:center;
	font-weight:bold;
	background:url(../imgs/Bg_Menu_Hover.gif) no-repeat;
	position:relative;
	height:36px;
}

.MenuChoise a
{
	display:block;
	color:#8D1E22;
	width:150px;
	text-decoration:none;
}

.SubMenuNotChoise
{
	line-height:32px;
	width:auto;
	display:none;
	height:32px; 
}

.SubMenuChoise a
{
	text-decoration:none;
}

.SubMenuChoise
{
	line-height:32px;
	width:auto;
	display:block;
	height:32px; 
}

.SubMenuNotChoise a,SubMenuNotChoise a:hover
{
	text-decoration:none;
	color:black;
}

.SubMenuNotChoise a:visited
{
	color:black;
	text-decoration:none;
}

.buttons{
	width:90px;
	height:26px;
	color:White;
	font-weight:bold;
	background:url(../imgs/Bg_btnYellow_Submit.gif) no-repeat;
	border-width:0px;
	text-align:center;
	cursor:hand;
}

.btnTable
{
	width:80px;
	height:20px;
	border:solid 1px #dc9701;
	background-color:white;
	color:#dc9701;
	line-height:16px;
	cursor:hand;
}

.btnBack
{
	width:60px;
	height:20px;
	border:solid 1px #dc9701;
	background-color:white;
	color:#dc9701;
	line-height:16px;
	cursor:hand;
}

