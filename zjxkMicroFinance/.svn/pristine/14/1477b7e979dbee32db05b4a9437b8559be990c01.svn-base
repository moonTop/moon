.dg
{	vertical-align:middle;
	border-spacing:0px;
	border: solid 1px #e0e0e0;
	width:100%;
	text-align:center;
}

.dgTitle
{
	text-align:center;
	border-width:0px;
	border-spacing:0px;
	background:url(../imgs/Bg_TableTitle_G.gif) repeat-x;
	color:white;
	border:none;
	height:20px;
	width:940px;
	border-left-width:0px;
	border-right-width:0px
}

.dgTitle td
{
	border-left-width:0px;
	border-right-width:0px;
}


.dgTitle th
{
	border-left-width:0px;
	border-right-width:0px;
}

.dgItem
{
	font-size:12px;
	color:#555555;
	background-color:White;
    border:solid 1px #e0e0e0;
    height:25px;
}

.dgItem td
{
	border-left-width:0px;
	border-right-width:0px;
	border-top:solid 1px #e0e0e0;
	border-bottom:solid 1px #e0e0e0;
	height:25px;

}

.dgAlterItem
{
	font-size:12px;
	color:#555555;
	background-color:#f6f5f5;
    border:solid 1px #e0e0e0;
    height:25px;
}

.dgAlterItem td
{
	border-left-width:0px;
	border-right-width:0px;
	border-top:solid 1px #e0e0e0;
	border-bottom:solid 1px #e0e0e0;
	height:25px;
}

