}

.dgPage a ,dgPage a :link,dgPage a :visited,dgPage a :hover
{
	 color:#555555;
	 font-size:13px;
}

.dgPage a:active
{
	color:#dc9701;
}

.dgPage span
{
	color:#dc9701;
}

.dgItem a ,dgItem a :link,dgItem a :visited,dgItem a :hover
{
